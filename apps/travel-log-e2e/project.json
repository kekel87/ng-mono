{
  "name": "travel-log-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "outputs": ["{workspaceRoot}/dist/.playwright/apps/travel-log-e2e"],
      "options": {
        "config": "apps/travel-log-e2e/playwright.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/travel-log-e2e/**/*.{js,ts}"],
        "cache": true,
        "cacheLocation": ".cache/eslint",
        "cacheStrategy": "content"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["travel-log"]
}
